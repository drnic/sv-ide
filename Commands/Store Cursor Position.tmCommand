<dict>
	<key>beforeRunningCommand</key>
	<string>nop</string>
	<key>command</key>
	<string>#!/usr/bin/env ruby

tmpdir = "/tmp"
require "fileutils"
FileUtils.mkdir_p tmpdir
tempfile = File.join(tmpdir, "textmate-current-position.csv")
File.open(tempfile, "w") do |f|
  f &lt;&lt; "#{ENV["TM_LINE_NUMBER"]},#{ENV["TM_LINE_INDEX"]}"
end</string>
